{
    "name": "jezzdk/fleet-cli",
    "description": "Run multiple Laravel Sail websites on your local environment.",
    "keywords": ["laravel", "console", "fleet", "cli", "sail", "docker", "traefik"],
    "homepage": "https://github.com/jezzdk/fleet-cli",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/jezzdk/fleet-cli/issues",
        "source": "https://github.com/jezzdk/fleet-cli"
    },
    "authors": [
        {
            "name": "Andrew Schmelyun",
            "email": "me@aschmelyun.com",
            "role": "Developer"
        },
        {
            "name": "Jess Stopa",
            "email": "jess@stopa.dk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "nunomaduro/termwind": "^1.15.1",
        "symfony/process": "^6.2",
        "symfony/yaml": "^6.2"
    },
    "require-dev": {
        "laravel-zero/framework": "^10.0.2",
        "laravel/pint": "^1.10",
        "mockery/mockery": "^1.5.1",
        "pestphp/pest": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": ["builds/fleet"]
}
